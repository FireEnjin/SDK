{
  "name": "@fireenjin/sdk",
  "version": "1.1.0",
  "private": false,
  "description": "Roll your own SDK",
  "main": "dist/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "rollup": "rollup -c rollup.config.js",
    "browser": "tsc --project tsconfig.browser.json",
    "test": "jest",
    "publish": "npm run build",
    "start": "tsc src/index.ts --outDir dist --declaration --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireenjin/sdk.git"
  },
  "author": "Madness Labs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fireenjin/sdk/issues"
  },
  "homepage": "https://github.com/fireenjin/sdk#readme",
  "dependencies": {
    "firebase": "^9.17.2",
    "graphql": "^15.8.0",
    "graphql-request": "^4.2.0",
    "graphql-tag": "^2.12.6",
    "localforage": "^1.10.0"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^29.4.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "rxjs": "^7.8.0",
    "semantic-release": "^20.1.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  }
}